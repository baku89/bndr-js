{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": ["./src/index.ts"],
	"readme": "./README.md",
	"out": "./static/docs",
	"excludePrivate": true,
	"excludeProtected": true,
	"excludeInternal": true,
	"entryPointStrategy": "expand",
	"groupOrder": [
		"Constructors",
		"Generators",
		"Combinators",
		"Filters",
		"Common Filters",
		"Methods",
		"Properties",
		"Global Functions",
		"Emitters"
	]
}
