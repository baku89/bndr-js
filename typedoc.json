{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": ["./src/index.ts"],
	"readme": "none",
	"out": "./docs/api",
	"plugin": ["typedoc-plugin-markdown"],
	"excludePrivate": true,
	"cleanOutputDir": true,
	"includeVersion": true,
	"hideInPageTOC": true,
	"groupOrder": [
		"Constructors",
		"Generators",
		"Combinators",
		"Filters",
		"Common Filters",
		"Methods",
		"Properties",
		"Global Functions",
		"Emitters"
	]
}
